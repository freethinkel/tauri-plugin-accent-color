{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "statusbar", "settings"],
  "platforms": ["macOS"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "webview:default",
    "window:allow-create",
    "window:allow-show",
    "window:allow-set-focus",
    "event:allow-listen",
    "event:allow-emit",
    "event:allow-emit-to",
    "window:allow-start-dragging",
    "webview:allow-set-webview-focus",
    "webview:allow-create-webview",
    "webview:allow-create-webview-window",
    "app:allow-version",
    "app:allow-tauri-version",
    "tray:allow-new",
    "tray:default",
    "accent-color:default",
    "accent-color:allow-get-accent-color"
  ]
}
